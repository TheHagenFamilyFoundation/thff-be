{
  "ServiceName": "thff-be-production",
  "SourceConfiguration": {
    "CodeRepository": {
      "RepositoryUrl": "https://github.com/TheHagenFamilyFoundation/thff-be",
      "SourceCodeVersion": {
        "Type": "BRANCH",
        "Value": "master"
      },
      "SourceDirectory": "/",
      "CodeConfiguration": {
        "ConfigurationSource": "REPOSITORY",
        "CodeConfigurationValues": {
          "Runtime": "NODEJS_18",
          "BuildCommand": "npm install",
          "StartCommand": "npm run production",
          "RuntimeEnvironmentVariables": {
            "NODE_ENV": "production",
            "PORT": "8080"
          }
        }
      }
    },
    "AutoDeploymentsEnabled": true,
    "AuthenticationConfiguration": {
      "ConnectionArn": "arn:aws:apprunner:us-east-1:759379104290:connection/thff-github/6a29e2d837a04986bafc7790cfca1e6c"
    }
  },
  "InstanceConfiguration": {
    "Cpu": "1024",
    "Memory": "2048",
    "InstanceRoleArn": "arn:aws:iam::759379104290:role/AppRunnerInstanceRole"
  }
}
